<script>
    document.addEventListener("DOMContentLoaded", function () {
    @if (TempData["SuccessMessage"] != null)
    {
        <text>
                Swal.fire({
                    icon: 'success',
                    title: 'Success!',
                    text: '@TempData["SuccessMessage"]',
                    timer: 5000,
                    showConfirmButton: false
                });
        </text>
    }

    @if (TempData["ErrorMessage"] != null)
    {
        <text>
                Swal.fire({
                    icon: 'error',
                    title: 'Error!',
                    text: '@TempData["ErrorMessage"]',
                    timer: 5000,
                    showConfirmButton: false
                });
        </text>
    }
                });
</script>